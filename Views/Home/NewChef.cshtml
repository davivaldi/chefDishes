@using ChefDishes
@model Chef



    <h1>Add a new Dish!</h1>
    <a href="/">Home</a>
    <form asp-action="CreateChef" asp-controller="Home" method="POST">
        <span asp-validation-for="FirstName"></span>
        <label asp-for="FirstName"></label><br>
        <input asp-for="FirstName"> <br><br>

        <span asp-validation-for="LastName"></span>
        <label asp-for="LastName"></label><br>
        <input asp-for="LastName"> <br><br>

        <span asp-validation-for="DOB"></span>
        <label asp-for="DOB"></label><br>
        <input asp-for="DOB"> <br><br>

        <input type="submit" value="Add Chef">
    </form>